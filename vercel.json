{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "runtime": "python3.7.4"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      }
    ]
  }
  